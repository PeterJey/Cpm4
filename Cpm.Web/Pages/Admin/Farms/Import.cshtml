@page
@model ImportModel
@{
    ViewData["Title"] = "Import";
}
<div class="page-header">
    <ol class="breadcrumb">
        <li><a asp-page="/Dashboard">Dashboard</a></li>
        <li><a asp-page="/Admin/Index">Admin</a></li>
        <li><a asp-page="/Admin/Users/Index">Farms</a></li>
        <li class="active">Import farm</li>
    </ol>
    <h2>@ViewData["Title"]</h2>
</div>
@Html.Raw(Model.LoadStatusHtml())
<form method="post" enctype="multipart/form-data">
    <div asp-validation-summary="All" class="text-danger"></div>
    <input asp-for="Input.Foo" hidden />
    <div class="form-group">
        <label asp-for="Input.File" class="control-label"></label>
        <input asp-for="Input.File" type="file" class="form-control" style="height: auto" />
        <span asp-validation-for="Input.File" class="text-danger"></span>
    </div>
    <input type="submit" value="Import" class="btn btn-success" />
</form>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
